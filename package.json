      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-plugin-prismjs": "^1.0.2",
    "babel-plugin-styled-components": "^1.10.2",
    "cross-env": "^5.2.0",
    "customize-cra": "^0.2.14",
    "gh-pages": "^2.0.1",
    "react-app-rewired": "^2.1.3",
    "react-snap": "^1.23.0"
  }
