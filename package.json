{
  "name": "redux-cheatsheet",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://wilsonwong.me/redux-cheatsheet",
  "dependencies": {
    "clipboard": "^2.0.4",
    "prismjs": "^1.27.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "styled-components": "^4.3.2"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/*.js",
    "start": "cross-env NODE_PATH=./ react-app-rewired start",
